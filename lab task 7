<body>
    <form action="#" class="form">
        <label for="#">Email</label>
        <input type="text" placeholder="email" class="email" name="email">

        <label for="#">Password</label>
        <input type="password" placeholder="password" class="password" name="password">

        <button onclick="Validation()">Submit</button>
    </form>



    <script>
        // alert('Welcome');
        // document.write("Helloe");
        // console.log("Hello");

        function Validation() {
            var d1 = document.form.email.value;
            var d2 = document.form.password.value;
            const email = "najeeb";
            const password = 1234;

            if (d1 == email && d2 == password) {
                alert("Login Success");
            }
            else {
                alert("Wrong Email or Password");
            }

        }
    </script>

    
